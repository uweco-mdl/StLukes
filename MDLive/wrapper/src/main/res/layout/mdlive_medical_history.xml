                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <RadioGroup
                                    android:id="@+id/primaryCareGroup"
                                    style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                                    android:orientation="horizontal">

                                    <RadioButton
                                        android:id="@+id/primaryCareYesButton"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="5dp"
                                        android:button="@drawable/behaviour_checked_selector"
                                        android:tag="@string/mdltag_yes" />

                                    <RadioButton
                                        android:id="@+id/primaryCareNoButton"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="5dp"
                                        android:button="@drawable/behaviour_uncheked_selector" />

                                </RadioGroup>
                            </LinearLayout>

                        </LinearLayout>
                        <View
